//Aqui se muestran las habitaciones creadas y se invcluye el boton para agregar más habitaciones
<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <title>Habitaciones</title>
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
//barra de navegacion
    <a href="/">Inicio</a>
    <h1>Habitaciones</h1>
//aqui se muestra la informacion y el boton para agregar ás informacion
    <ul>
        <% habitaciones.forEach (habitacion => { %>

            <a href="/hbtcns/<%= habitacion.id %>"> <li><%= habitacion.noHabitacion%></li> </a>

        <% });%>
    </ul>
    <a href="/hbtcns/create80">Agregar nueva habitación</a>
</body>
</html>
